<!DOCTYPE html>
<html>
  <head>
    <%= yield :meta_title_and_keywords %>
    <%= stylesheet_link_tag 'style', 'facebox', 'jquery-ui-1.8.4.custom' %>

    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="main">
      <%= render :partial => 'shared/header' %>
      <div id="wrapper">
        <div class="container left">
          <h2> <%= yield :heading %> </h2>
          <%= yield %>
        </div>
        <%= render :partial => '/shared/sidebar'%>
        <div class="clear"></div>
      </div>
    </div>
    <%= render :partial => 'shared/footer'%>
  </body>

  <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js' %>
  <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.10/jquery-ui.min.js' %>
  <%= javascript_include_tag 'https://github.com/vijendra/ivmu/blob/master/public/javascripts/rails.js' %>
  <%= javascript_include_tag 'https://github.com/vijendra/ivmu/blob/master/public/javascripts/jHtmlArea.js' %>
  <%= javascript_include_tag 'https://github.com/vijendra/ivmu/blob/master/public/javascripts/facebox.js' %>
  <%= javascript_include_tag 'https://github.com/vijendra/ivmu/blob/master/public/javascripts/application.js' %>


</html>

